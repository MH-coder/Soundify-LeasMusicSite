<!--Variety music-->
<!DOCTYPE html>
<html class="wide wow-animation" lang="en">

<head>
  <title>Home</title>
  <meta name="format-detection" content="telephone=no">
  <meta name="viewport"
    content="width=device-width, height=device-height, initial-scale=1.0, maximum-scale=1.0, user-scalable=0">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta charset="utf-8">
  <script src="/cdn-cgi/apps/head/3ts2ksMwXvKRuG480KNifJ2_JNM.js"></script>
  <link rel="icon" href="images/favicon.ico" type="image/x-icon">
  <!-- Stylesheets-->
  <link rel="stylesheet" type="text/css" href="//fonts.googleapis.com/css?family=Roboto:100,300,300i,400,700,900">
  <link rel="stylesheet" href="//cdn.jsdelivr.net/npm/@mdi/font@6.5.95/css/materialdesignicons.min.css">
  <link rel="stylesheet" href="https://cdn.linearicons.com/free/1.0.0/icon-font.min.css">
  <link rel="stylesheet" href="css/bootstrap.css">
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.0.0/css/bootstrap.min.css"
    integrity="sha512-k78e1fbYs09TQTqG79SpJdV4yXq8dX6ocfP0bzQHReQSbEghnS6AQHE2BbZKns962YaqgQL16l7PkiiAHZYvXQ=="
    crossorigin="anonymous" referrerpolicy="no-referrer" />
  <link rel="stylesheet" href="css/fonts.css">
  <link rel="stylesheet" href="css/style.css" id="main-styles-link">

</head>

<body>
  <div class="preloader">
    <div class="preloader-body">
      <div class="cssload-container">
        <div class="cssload-speeding-wheel"></div>
      </div>
      <p>Loading...</p>
    </div>
  </div>
  <div class="page">
    <!-- Page Header-->
    <header class="section page-header">
      {{>navbar}}
    </header>
    <!-- Swiper-->
    <section class="section swiper-container swiper-slider swiper-slider-2" data-loop="false" data-autoplay="false"
      data-simulate-touch="false" data-slide-effect="fade">
      <div class="swiper-wrapper">
        <div class="swiper-slide context-dark" data-slide-bg="images/bg4.jpg">
          <div class="swiper-slide-caption">
            <div class="container">
              <div class="row justify-content-lg-left">
                <div class="col-md-12 col-xl-10">
                  <h1 data-caption-animate="fadeInUp" data-caption-delay="100">Offering the Best Music to Listeners
                    Worldwide</h1><a class="button button-lg button-primary" href="/about"
                    data-caption-animate="fadeInUp" data-caption-delay="450">Learn More</a>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>

    </section>

    <!-- Variety of Music Genres-->
    <section class="section section-lg">
      <div class="container">
        <div class="block-lg text-center">
          <h2>A Variety of Music Genres</h2>
          <p>We offer our listeners a wide variety of music genres, from House and Dubstep to Rock and Jazz. Explore
            music and that we have to offer you below.</p>
        </div>
        <div class="row row-30 justify-content-center">
          <div class="col-sm-12 col-md-6 col-lg-4 col-xl-3 wow fadeInLeft"><span class="variety-box-wrapper">
              <figure class="variety-box">
                <div class="variety-media"><img src="images/music-style-1-49x49.png" alt="" width="49" height="49" />
                </div>
                <figcaption><span>House</span>
                  <p>We pick the best music for our listeners.</p>
                </figcaption>
              </figure>
            </span>
          </div>
          <div class="col-sm-12 col-md-6 col-lg-4 col-xl-3 wow fadeInLeft"><span class="variety-box-wrapper">
              <figure class="variety-box">
                <div class="variety-media"><img src="images/music-style-2-47x56.png" alt="" width="47" height="56" />
                </div>
                <figcaption><span>Drum &amp; Bass</span>
                  <p>Enjoy top-notch Liquid D&amp;B and Neurofunk music.</p>
                </figcaption>
              </figure>
            </span>
          </div>
          <div class="col-sm-12 col-md-6 col-lg-4 col-xl-3 wow fadeInLeft"><span class="variety-box-wrapper">
              <figure class="variety-box">
                <div class="variety-media"><img src="images/music-style-3-37x46.png" alt="" width="37" height="46" />
                </div>
                <figcaption><span>Dubstep</span>
                  <p>Dozens of new Dubstep tracks</p>
                </figcaption>
              </figure>
            </span>
          </div>
          <div class="col-sm-12 col-md-6 col-lg-4 col-xl-3 wow fadeInLeft"><span class="variety-box-wrapper">
              <figure class="variety-box">
                <div class="variety-media"><img src="images/music-style-4-53x45.png" alt="" width="53" height="45" />
                </div>
                <figcaption><span>Trance</span>
                  <p>Our DJs broadcast various Trance tracks.</p>
                </figcaption>
              </figure>
            </span>
          </div>
          <div class="col-sm-12 col-md-6 col-lg-4 col-xl-3 wow fadeInLeft"><span class="variety-box-wrapper">
              <figure class="variety-box">
                <div class="variety-media"><img src="images/music-style-5-51x48.png" alt="" width="51" height="48" />
                </div>
                <figcaption><span>Breaks</span>
                  <p>Listen to the top-notch Breaks selection.</p>
                </figcaption>
              </figure>
            </span>
          </div>
          <div class="col-sm-12 col-md-6 col-lg-4 col-xl-3 wow fadeInLeft"><span class="variety-box-wrapper">
              <figure class="variety-box">
                <div class="variety-media"><img src="images/music-style-6-31x50.png" alt="" width="31" height="50" />
                </div>
                <figcaption><span>Techno</span>
                  <p>Discover all subgenres of legendary Techno.</p>
                </figcaption>
              </figure>
            </span>
          </div>
          <div class="col-sm-12 col-md-6 col-lg-4 col-xl-3 wow fadeInLeft"><span class="variety-box-wrapper">
              <figure class="variety-box">
                <div class="variety-media"><img src="images/music-style-7-25x63.png" alt="" width="25" height="63" />
                </div>
                <figcaption><span>Rock</span>
                  <p>Weekly concerts and rock exclusives</p>
                </figcaption>
              </figure>
            </span>
          </div>
          <div class="col-sm-12 col-md-6 col-lg-4 col-xl-3 wow fadeInLeft"><span class="variety-box-wrapper">
              <figure class="variety-box">
                <div class="variety-media"><img src="images/music-style-8-39x54.png" alt="" width="39" height="54" />
                </div>
                <figcaption><span>Jazz</span>
                  <p>Best Jazz music for our listeners.</p>
                </figcaption>
              </figure>
            </span>
          </div>
        </div>
      </div>
    </section>
    <!--	Schedule of Events-->
    <section class="section section-lg bg-gray-100">
      <div class="container text-center">
        <h2>Schedule of Events</h2>
        <p>We try to be in touch with our lovers through our concerts. They include exclusive guest mixes,live<br
            class="d-none d-lg-block">performances, interviews, and appearances by world-famous DJs and musicians</p>
        <div class="row row-30 row-offset-4 text-left">
          {{#each post}}
          <div class="col-md-6 col-lg-4 wow fadeInRight">
            <article class="post-boxed">
              <div class="post-meta">
                <div class="post-meta-item">
                  <div class="post-author"><span>by</span> <span>{{this.artist}}</span>
                  </div>
                </div>
                <div class="post-meta-item">
                  <div class="post-date">{{this.date}}</div>
                </div>
              </div>
              <a class="media-wrapper" href="#">
                <img src="https://source.unsplash.com/1600x900/?music" alt="" width="370" height="100" />
              </a>
              <div class="post-body">
                <h6 class="post-title"><a href="#">{{this.eventName}}</a></h6>
                <p class="post-exeption">{{this.eventDesc}}</p>
                <form action="/reserveSeat" method="post">
                  <input type="text" value={{this._id}} style="display: none;" name="eventId">

                  <input type="text" value={{this.eventName}} style="display: none;" name="eventName">

                  <input type="text" value={{this.eventStatus}} style="display: none;" name="eventName">

                  {{#if (isCancelled this.eventStatus)}}
                  <button type="submit" class="btn btn-dark mt-3">Join</button>
                  <p class="post-exeption" style="color: gray; font-size:x-small">Register yourself for this event</p>
                  {{else}}
                  <p class="post-exeption" style="color: gray; font-size:medium">Event is cancelled!</p>
                  {{/if}}
                </form>

              </div>
            </article>
          </div>
          {{/each}}
        </div>
        <div class="button-wrap-lg"><a class="button button-lg button-primary" href="/events">View My Joined Events</a></div>
      </div>
    </section>
    
    {{>footer}}
  </div>

  <!-- Javascript-->
  <script src="js/core.min.js"></script>
  <script src="js/script.js"></script>
</body>

</html>